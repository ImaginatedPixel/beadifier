    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build -prod",
    "serve": "cd dist && http-server .",
    "expose": "ngrok http 8080",
    "docker:stop": "docker stop beadifier || true",
    "docker:rm": "docker rm beadifier || true",
    "docker:build": "yarn build:prod && docker build -t beadifier:latest .",
    "docker:start": "yarn docker:stop && yarn docker:rm && yarn docker:build && docker run --name beadifier -d -p 5555:80 beadifier:latest"
